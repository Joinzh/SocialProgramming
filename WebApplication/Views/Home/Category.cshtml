@model IEnumerable<WebApplication.Entry>
@if (Culture != "hu-HU")
{
    ViewBag.Title = ((Category)ViewBag.Category).enTitle;
    <div class="accordion_container" style="min-height: 38px;">
        <h2 class="accordion_panel active">
            @(((Category)ViewBag.Category).enTitle)</h2>
    </div>
}
else
{
    ViewBag.Title = ((Category)ViewBag.Category).huTitle;
    <div class="accordion_container" style="min-height: 38px;">
        <h2 class="accordion_panel active">
            @(((Category)ViewBag.Category).huTitle)</h2>
    </div> 
}

@Html.Partial("_entriesList", Model)

@if (Model.Count() == (int)ViewBag.max)
{
<div class="loader" id="#loadMore">
    <div class="l"><input type="hidden" value="@string.Join(",", Model.Select(e => e.Id).ToArray())" id="entry-ids" /></div>
</div>
}